<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="3.0">
    <display-name>HITgram-WebApp</display-name>
    <servlet>
        <servlet-name>ChatbotServlet</servlet-name>
        <servlet-class>com.ngram.HITgram.ChatbotServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChatbotServlet</servlet-name>
        <url-pattern>/chatbot</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
